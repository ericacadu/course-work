            <input type="email" class="form-control m-0 mb-3" id="username" placeholder="bistro@gmail.com"
            value="bistro@gmail.com">
            <label for="username">Email</label>
        </div>
        <div class="form-floating">
            <input type="password" class="form-control m-0" id="password" placeholder="bistro" value="bistro">
            <label for="password">Password</label>
        </div>
        <button class="btn w-50 mt-3 mx-auto btn-primary rounded-3" id="signinBtn">
            登入
        </button>
    </div>
</div>
